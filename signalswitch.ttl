@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix atom: <http://lv2plug.in/ns/ext/atom#> .
@prefix midi: <http://lv2plug.in/ns/ext/midi#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<http://example.org/plugins/signalswitch>
    a lv2:Plugin ;
    lv2:project [
        a doap:Project ;
        doap:name "SignalSwitchLV2" ;
    ] ;
    lv2:optionalFeature lv2:hardRTCapable ;
    rdfs:label "Signal Switcher" ;

    lv2:port [
        a lv2:AudioPort , lv2:InputPort ;
        lv2:index 0 ;
        lv2:symbol "input" ;
        rdfs:label "Audio Input"
    ] ,
    [
        a atom:AtomPort , lv2:InputPort ;
        atom:bufferType atom:Sequence ;
        atom:supports midi:MidiEvent ;
        lv2:index 1 ;
        lv2:symbol "midi_in" ;
        rdfs:label "MIDI Input"
    ] ,
    [
        a lv2:ControlPort , lv2:InputPort ;
        lv2:index 2 ;
        lv2:symbol "cc_number" ;
        rdfs:label "MIDI CC Number" ;
        lv2:default 20 ;
        lv2:minimum 0 ;
        lv2:maximum 127
    ] ,
    [
        a lv2:ControlPort , lv2:InputPort ;
        lv2:index 3 ;
        lv2:symbol "midi_channel" ;
        rdfs:label "MIDI Channel" ;
        lv2:default 0 ;
        lv2:minimum 0 ;
        lv2:maximum 15
    ] .
